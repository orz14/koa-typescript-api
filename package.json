{
  "name": "koa-typescript",
  "version": "1.0.0",
  "description": "Koa.js TypeScript REST API",
  "main": "src/app.ts",
  "scripts": {
    "start": "nodemon"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node ./src/app.ts"
  },
  "keywords": [
    "koajs",
    "typescript",
    "prisma",
    "jwt"
  ],
  "author": "orzcode",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.1",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.15.0",
    "koa-bodyparser": "^4.4.1",
    "koa-router": "^12.0.1",
    "ts-node": "^10.9.2",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/koa": "^2.14.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-router": "^7.4.8",
    "@types/node": "^20.11.16",
    "nodemon": "^3.0.3",
    "prisma": "^5.9.1",
    "typescript": "^5.3.3"
  }
}
